= form_with scope: :bicycle, url: bicycles_path, local: true do |f|
  %p
    = f.label :Name
    %br
    = f.text_field :name
  %p
    = f.label :Description
    %br
    = f.text_area :description
  %p

  %p
    = f.label :Image
    %br
    = f.file_field :image
  %p
    = f.submit 'Submit', class: "btn btn-primary"